{
  "manifest_version": 2,
  "name": "Twitchy",
  "version": "0.1",
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "webRequest",
    "webNavigation",
    "webRequestBlocking"
    ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": ["popoutStyles.css"],
      "js": ["jquery-3.2.1.min.js", "jquery-ui.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "web_accessible_resources": ["back.png", "cancel.png", "home.png", "heart.png"]
}
